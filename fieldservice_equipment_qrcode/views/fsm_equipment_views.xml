<?xml version="1.0" encoding="utf-8" ?>
<odoo>
<record id="fsm_equipment_calibration_certificate_form_view_inherit" model="ir.ui.view">
    <field name="name">fsm.calibration.certificate.inherit</field>
    <field name="model">fsm.equipment</field>
    <field name="inherit_id" ref="fieldservice.fsm_equipment_form_view" />
    <field name="arch" type="xml">
        <notebook position="inside">
            <page string="Calibration Certificate">
                <field name="fsm_calibration_certificate_ids">
                    <tree>
                        <field name="name"/>
                        <field name="initial_date" />
                        <field name="expiration_date" />
                        <field name="fsm_equipment_id" />
                    </tree>
                </field>
            </page>
        </notebook>
    </field>
</record>

</odoo>
